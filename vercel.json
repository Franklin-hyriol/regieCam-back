{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ],
    "scripts": {
        "build": "npm run build",
        "start": "node dist/index.js"
    }
}